  "scripts": {
    "start": "ng serve --port 8888 --hmr",
    "debug": "node --nolazy node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.json --port 8888 --hmr --configuration=hmr --common-chunk --vendor-chunk --source-map",
    "aot": "node --max_old_space_size=5120 node_modules/@angular/cli/bin/ng serve --proxy-config proxy.conf.json --port 8888 --aot --hmr --configuration=hmr --source-map",
    "build:prod": "node --max_old_space_size=5120 node_modules/@angular/cli/bin/ng build --output-path=build-prod --delete-output-path --prod --build-optimizer --watch --common-chunk --vendor-chunk --stats-json",
    "build:prod:2": "node --max_old_space_size=5120 node_modules/@angular/cli/bin/ng build --output-path=build-prod --delete-output-path --prod --build-optimizer=false --watch --common-chunk --vendor-chunk --stats-json",
    "build:aot": "node --max_old_space_size=5120 node_modules/@angular/cli/bin/ng  build --aot --output-path=build-aot --delete-output-path --watch --common-chunk --vendor-chunk --stats-json",
    "stats:prod": "npx webpack-bundle-analyzer prod/stats.json --port 8889",
    "stats:aot": "npx webpack-bundle-analyzer build-aot/stats.json --port 8886",
    "webpack-server": "webpack-dev-server --port 8888 --inline --progress --content-base src --colors --hot --devtool eval",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
